{
  "name": "cmg-syntax",
  "displayName": "CMG Syntax",
  "description": "CMG simulation files syntax highlighting",
  "repository": "https://github.com/TiagoCAAmorim/cmg-syntax",
	"publisher": "tiagocaamorim",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "keywords": [
    "cmg",
    "imex",
    "gem",
    "simulator"
  ],
  "contributes": {
    "languages": [{
      "id": "cmg",
      "aliases": ["CMG", "cmg"],
      "extensions": [".dat",".inc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cmg",
      "scopeName": "source.cmg",
      "path": "./syntaxes/cmg.tmLanguage.json"
    }]
  }
}

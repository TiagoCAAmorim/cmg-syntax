{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "CMG",
	"patterns": [
		{"include": "#comments"},
		{"include": "#triggers"},
		{"include": "#strings"},
		{"include": "#numbers"},
		{"include": "#keywords"},
		{"include": "#tests"}
	],
	"repository": {
		"comments": {
			"patterns": [{
				"name": "comment",
				"match": "\\*\\*.*$\n?"
			},{
				"name": "comment",
				"match": "^\\*?RESULTS.*$\n?"
			}]
		},

		"triggers": {
			"patterns": [{
				"name": "variable.parameter",
				"match": "(?<=TRGVARS?)\\s+'[^']+'(?=(\\s|$))"
			},{
				"name": "support.type",
				"match": "^\\s*\\*?TRGVARS?(?=\\s+'[^']+'(\\s|$))"
			},{
				"name": "variable.parameter",
				"match": "(?<='VARSUB:)[^']+'"
			},{
				"name": "support.type",
				"match": "(?<=')VARSUB:"
			},{
				"name": "variable.parameter",
				"match": "\\s'(?=VARSUB:)"
			},{
				"name":"variable.language",
				"match":"^\\s*\\*?(TRIGGER|END_TRIGGER)(?=(\\s|$))"
			}]
		},

		"strings": {
			"name": "string.quoted.double.cmg",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.cmg",
					"match": ""
				}
			]
		},

		"numbers": {
			"patterns": [{
				"name": "keyword.operator",
				"match": "(?<=\\s)([=+\\-\\\\\\*/><]|\\*?(AND|OR)\\d*)(?=\\s)"
			},{
				"name": "token.warn-token",
				"match": "(^|\\s)\\d+(:\\d+)?\\s+\\d+(:\\d+)?\\s+\\d+(:\\d+)?(?=(\\s|$))"
			},{
				"name": "token.warn-token",
				"match": "(^|\\s)\\d+(:\\d+)?,\\d+(:\\d+)?,\\d+(:\\d+)?(?=(\\s|$))"
			},{
				"name": "constant.numeric",
				"match": "(?<=(\\s|^))([-+]|[0-9]+\\*)?([0-9]*\\.?[0-9]+|[0-9]+\\.)([eE][-+]?[0-9]+)?(?=(\\s|$))"
			}]
		},

		"keywords": {
			"patterns": [
				{ "include": "#main-keyword" }, 
				{ "include": "#include" }, 
				{ "include": "#date" }, 
				{ "include": "#special-keyword" },
				{ "include": "#keyword" },
				{ "include": "#option" }]
		},

		"main-keyword": {
			"patterns": [{
				"name": "token.error-token",
				"match": "^\\s*\\*?(TITLE1|GRID|MODEL|ROCKFLUID|INITIAL|NUMERICAL|RUN|STOP)(?=(\\s*$|\\s+\\*\\*))"
			}]
		},

		"include": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "^\\s*\\*?INCLUDE\\s+'[^']+'(?=(\\s*$|\\s+\\*\\*))"
			}]
		},

		"date": {
			"patterns": [{
				"name": "constant.character",
				"match": "^\\s*\\*?(DATE\\s+\\d{4}\\s+\\d{1,2}\\s+([0-9]*\\.?[0-9]+|[0-9]+\\.)([eE][-+]?[0-9]+)?|TIME\\s+\\+?([0-9]*\\.?[0-9]+|[0-9]+\\.)([eE][-+]?[0-9]+)?)(?=(\\s*$|\\s+\\*\\*))"
			}]
		},
				
		"special-keyword": {
			"patterns": [{
				"name":"entity.name.function",
				"match":"\\s\\*?OPEN(?=(\\s|$))"
			},{
				"name":"variable.language",
				"match":"^\\s*\\*?CLOSED(?=(\\s|$))"
			},{			
				"name":"variable.language",
				"match":"(?<=\\s)\\*?CLOSED(?=(\\s|$))"
			}]
		},
			
		"keyword": {
			"patterns": [{
					"name": "entity.name.function",
					"match": "^\\s*\\*?(3PTSCALING|3PTSCALING_KRG|3PTSCALING_KROW|3PTSCALING_KRW|3PTSCAL_KRG|3PTSCAL_KROG|3PTSCAL_KROW|3PTSCAL_KRW|3PTSCAL_PCOG|3PTSCAL_PCOW)(?=(\\s|$))"
